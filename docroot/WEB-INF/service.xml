<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE service-builder PUBLIC "-//Liferay//DTD Service Builder 6.0.0//EN" "http://www.liferay.com/dtd/liferay-service-builder_6_0_0.dtd">
<service-builder package-path="org.gnenc.dlusagereports">
	<author>Drew A. Blessing</author>
	<namespace>DLUsage</namespace>

	<entity name="AllocatedStorage" 
			local-service="true" remote-service="false" 
			human-name="AllocatedStorage" json-enabled="false">

		<!-- PK fields -->

		<column name="id" type="long" primary="true" />

		<!-- Audit fields -->

		<column name="totalBytes" type="long" />
		<column name="ModifiedUserId" type="long" />
		<column name="modifiedDate" type="Date" />

		<!-- Order -->

		<order by="asc">
			<order-column name="id" />
		</order>

		<!-- Finder methods -->
	</entity>
</service-builder>
